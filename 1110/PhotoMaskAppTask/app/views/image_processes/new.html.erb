<h2>📷 증명사진 배경 투명화</h2>
<%= form_with(model: @image_result, url: image_processes_path, local: true, html: { multipart: true }) do |form| %>
  
  <div class="field">
    <%= form.label :original_image, "증명사진 파일 선택 (JPG/PNG)" %><br>
    <%= form.file_field :original_image %>
  </div>

  <div class="actions" style="margin-top: 20px;">
    <%= form.submit "배경 투명화 처리", class: "btn btn-primary" %>
  </div>
<% end %>